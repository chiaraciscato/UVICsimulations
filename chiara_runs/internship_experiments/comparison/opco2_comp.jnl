		use /Users/chiaraciscato/Desktop/geomar/UVic/chiara_runs/internship_experiments/ccn_experiments/rcp85-50/tavg.01955.01.01.nc
		use /Users/chiaraciscato/Desktop/geomar/UVic/chiara_runs/internship_experiments/ccn_experiments/rcp85-85/tavg.01955.01.01.nc

		use /Users/chiaraciscato/Desktop/geomar/UVic/chiara_runs/internship_experiments/emiss_experiments/rcp85/tavg.01955.01.01.nc
		use /Users/chiaraciscato/Desktop/geomar/UVic/chiara_runs/internship_experiments/emiss_experiments/rcp85-25/tavg.01955.01.01.nc
		use /Users/chiaraciscato/Desktop/geomar/UVic/chiara_runs/internship_experiments/emiss_experiments/rcp85-50/tavg.01955.01.01.nc
		use /Users/chiaraciscato/Desktop/geomar/UVic/chiara_runs/internship_experiments/emiss_experiments/rcp85-85/tavg.01955.01.01.nc


! define variables 


		let pco2_rcp85 = O_PCO2SUR[d=1, l=30]
		let pco2_rcp8525 = O_PCO2SUR[d=2, l=30]
		let pco2_rcp8550 = O_PCO2SUR[d=3, l=30]
		let pco2_rcp8585 = O_PCO2SUR[d=4, l=30]

		let pco2_cng_25 = pco2_rcp85 - pco2_rcp8525
		let pco2_cng_50 = pco2_rcp85 - pco2_rcp8550
		let pco2_cng_85 = pco2_rcp85 - pco2_rcp8585

		let emis_pco2_rcp85 = O_PCO2SUR[d=5, l=30]
		let emis_pco2_rcp8525 = O_PCO2SUR[d=6, l=30]
		let emis_pco2_rcp8550 = O_PCO2SUR[d=7, l=30]
		let emis_pco2_rcp8585 = O_PCO2SUR[d=8, l=30]

		let emis_pco2_cng_25 = emis_pco2_rcp85 - emis_pco2_rcp8525
		let emis_pco2_cng_50 = emis_pco2_rcp85 - emis_pco2_rcp8550
		let emis_pco2_cng_85 = emis_pco2_rcp85 - emis_pco2_rcp8585

		let comp_25 = pco2_cng_25 - emis_pco2_cng_25
		let comp_50 = pco2_cng_50 - emis_pco2_cng_50
		let comp_85 = pco2_cng_85 - emis_pco2_cng_85


! set plot routine


		set windows/new
		set text/font='Arial'
		set text/isiz=4
		go portrait1x2.jnl


		define view/xlimits=0.07,0.9/ylimits=0.6,1 tl
		define view/xlimits=0.07,0.9/ylimits=0.3,0.7 ml
		define view/xlimits=0.07,0.9/ylimits=0,0.4 bl
		

		set view tl
		shade/nolabel/pal= cmocean_matter/levels="(-inf)(0,10,1)(inf)" comp_25
		go land
		label/nouser 5.5,3.2,1,0,0.11 "RCPmed - Ocean <#916>pCO<sub>2</sub> [ppm]"

		set view ml
		shade/nolabel/pal= cmocean_matter/levels="(-inf)(0,10,1)(inf)" comp_50
		go land
		label/nouser 5.5,3.2,1,0,0.11 "RCPlow - Ocean <#916>pCO<sub>2</sub> [ppm]"

		set view bl
		shade/nolabel/pal= cmocean_matter/levels="(-inf)(0,10,1)(inf)" comp_85
		go land
		label/nouser 5.5,3.2,1,0,0.11 "RCPvl - Ocean <#916>pCO<sub>2</sub> [ppm]"


