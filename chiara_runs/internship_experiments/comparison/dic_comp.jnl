! define variables 


		let odic_rcp85 = O_DIC[d=1, z=1, l=29]
		let odic_rcp8525 = O_DIC[d=2, z=1, l=29]
		let odic_rcp8550 = O_DIC[d=3, z=1, l=29]
		let odic_rcp8585 = O_DIC[d=4, z=1, l=29]

		let odic_cng_25 = odic_rcp85 - odic_rcp8525
		let odic_cng_50 = odic_rcp85 - odic_rcp8550
		let odic_cng_85 = odic_rcp85 - odic_rcp8585

		let emis_odic_rcp85 = O_DIC[d=5, z=1, l=29]
		let emis_odic_rcp8525 = O_DIC[d=6, z=1, l=29]
		let emis_odic_rcp8550 = O_DIC[d=7, z=1, l=29]
		let emis_odic_rcp8585 = O_DIC[d=8, z=1, l=29]

		let emis_odic_cng_25 = emis_odic_rcp85 - emis_odic_rcp8525
		let emis_odic_cng_50 = emis_odic_rcp85 - emis_odic_rcp8550
		let emis_odic_cng_85 = emis_odic_rcp85 - emis_odic_rcp8585

		let comp_25 = odic_cng_25 - emis_odic_cng_25
		let comp_50 = odic_cng_50 - emis_odic_cng_50
		let comp_85 = odic_cng_85 - emis_odic_cng_85


! set plot routine


		set windows/new
		set text/font='Arial'
		set text/isiz=4
		go portrait1x2.jnl


		define view/xlimits=0.1,0.85/ylimits=0.6,1 tl
		define view/xlimits=0.1,0.85/ylimits=0.3,0.7 ml
		define view/xlimits=0.1,0.85/ylimits=0,0.4 bl
		

	set view tl
	shade/nolabel/pal=cmocean_curl/levels="(-inf)(-0.1e-02,0.1e-02,0.15e-03)(inf)" 	comp_25 	
	go land
	label/nouser 4.5,3.4,1,0,0.11 "DIC [mol m<sup>-3</sup>] - RCPmed"
	label 0,105,0,0,0.12 "(a)"

	set view ml
	shade/nolabel/pal= cmocean_curl/levels="(-inf)(-0.1e-02,0.1e-02,0.15e-03)(inf)" comp_50
	go land
	label/nouser 4.5,3.4,1,0,0.11 "DIC [mol m<sup>-3</sup>] - RCPlow"
	label 0,105,0,0,0.12 "(b)"

	set view bl
	shade/nolabel/pal= cmocean_curl/levels="(-inf)(-0.1e-02,0.1e-02,0.15e-03)(inf)" comp_85
	go land
	label/nouser 4.5,3.4,1,0,0.11 "DIC [mol m<sup>-3</sup>] - RCPvl"
	label 0,105,0,0,0.12 "(c)"


